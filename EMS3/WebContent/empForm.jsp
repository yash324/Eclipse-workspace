<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><%@page errorPage="WEB-INF/showErrorMessage.jsp"%><html><head><link rel="stylesheet" type="text/css" href="default.css"></head><body>	<p>		<a href="controller?action=viewEmpList">[Return to List]</a>	</p>	<form method="post" action="controller?action=saveEmp">		<input type="hidden" name="empid" value="${emp.empid}" />		<table>			<tr>				<td>Full Name<font color="red"><sup>*</sup></font>				</td>				<td><input type="text" name="name" value="${emp.name}" pattern = "[A-Za-z]+ [A-Za-z]+" required/></td>			</tr>			<tr>				<td>Email</td>				<td><input type="text" name="email" value="${emp.email}" pattern = "[A-Za-z0-9_]+@[A-Za-z0-9_.]+" required/></td>			</tr>			<tr>				<td>Phone</td>				<td><input type="text" name="phone" value="${emp.phone}" pattern = "[1-9][0-9]{9}" required/></td>			</tr>			<tr>				<td>Date of Birth</td>				<td><input type="date" name="dob" value="${emp.dob}" required/> </td>			</tr>			<tr>				<td>Date of Joining</td>				<td><input type="date" name="doj" value="${emp.doj}" required/></td>			</tr>			<tr>				<td>Address</td>				<td><input type="text" name="address" value="${emp.address}" required/></td>			</tr>			<tr>				<td>Department</td>				<td><input type="text" name="deptid" value="${emp.deptid}" required/></td>			</tr>			<tr>				<td>Project</td>				<td><input type="text" name="projid" value="${emp.projid}" required/></td>			</tr>			<tr>				<td>Role</td>				<td><input type="text" name="roleid" value="${emp.roleid}" required/></td>			</tr>			<tr>				<td colspan="2"><input type="submit" name="save" value="Save" />					&nbsp;&nbsp; <input type="reset" name="reset" value="Reset" /></td>			</tr>		</table>	</form></body></html>