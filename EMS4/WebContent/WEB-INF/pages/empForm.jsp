<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><%@ taglib prefix="form" uri="http://www.springframework.org/tags/form"%><%@page errorPage="WEB-INF/pages/showErrorMessage.jsp"%><html><head><link rel="stylesheet" type="text/css" href="default.css"></head><body>	<p>		<a href="${pageContext.request.contextPath}/">[Return to List]</a>	</p>	<form:form method="post" action="${pageContext.request.contextPath}/saveEmp" commandName="emp">		<form:hidden path="empid"/>		<table>			<tr>				<td>Full Name</td>				<td><form:input path = "name" pattern = "[A-Za-z]+ [A-Za-z]+" oninvalid = "this.setCustomValidity('Use format: First_Name Last_Name')" /></td>			</tr>			<tr>				<td>Email</td>				<td><form:input type="email" path ="email"/></td>			</tr>			<tr>				<td>Phone</td>				<td><form:input path ="phone" pattern = "[1-9][0-9]{9}" oninvalid = "this.setCustomValidity('Use 10 digit phone number not beginning with 0')"/></td>			</tr>			<tr>				<td>Date of Birth</td>				<td><form:input path="dob" pattern = "[0-9]{2}/[0-9]{2}/[0-9]{4}" oninvalid = "this.setCustomValidity('Use mm/dd/yyyy only.')"/> </td>			</tr>			<tr>				<td>Date of Joining</td>				<td><form:input path = "doj" pattern = "[0-9]{2}/[0-9]{2}/[0-9]{4}" oninvalid = "this.setCustomValidity('Use mm/dd/yyyy only.')"/></td>			</tr>			<tr>				<td>Address</td>				<td><form:input path ="address"/></td>			</tr>			<tr>				<td>Department</td>				<td><form:input path="deptid" /></td>			</tr>			<tr>				<td>Project</td>				<td><form:input path="projid" /></td>			</tr>			<tr>				<td>Role</td>				<td><form:input path="roleid" /></td>			</tr>			<tr>				<td colspan="2"><input type="submit" name="save" value="Save" />					&nbsp;&nbsp; <input type="reset" name="reset" value="Reset" /></td>			</tr>		</table>	</form:form></body></html>