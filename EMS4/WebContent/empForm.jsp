<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><%@ taglib prefix="form" uri="http://www.springframework.org/tags/form"%><%@page errorPage="WEB-INF/showErrorMessage.jsp"%><html><head><link rel="stylesheet" type="text/css" href="default.css"></head><body>	<p>		<a href="controller?action=viewEmpList">[Return to List]</a>	</p>	<form:form method="post" action="controller?action=viewEmp" commandName="emp">		<form:hidden path="empid"/>		<table>			<tr>				<td>Full Name<font color="red"><sup>*</sup></font></td>				<td><form:input path = "name" pattern = "[A-Za-z]+ [A-Za-z]+" oninvalid = "this.setCustomValidity('Use format: First_Name Last_Name')" /></td>			</tr>			<tr>				<td>Email</td>				<td><form:input path ="email" pattern = "[A-Za-z0-9_]+@[A-Za-z0-9_.]+"/></td>			</tr>			<tr>				<td>Phone</td>				<td><form:input path ="phone" pattern = "[1-9][0-9]{9}" oninvalid = "this.setCustomValidity('Use 10 digit phone number not beginning with 0')"/></td>			</tr>			<tr>				<td>Date of Birth</td>				<td><form:input path="dob"/> </td>			</tr>			<tr>				<td>Date of Joining</td>				<td><form:input path = "doj"/></td>			</tr>			<tr>				<td>Address</td>				<td><form:input path ="address"/></td>			</tr>			<tr>				<td>Department</td>				<td><form:input path="deptid" /></td>			</tr>			<tr>				<td>Project</td>				<td><form:input path="projid" /></td>			</tr>			<tr>				<td>Role</td>				<td><form:input path="roleid" /></td>			</tr>			<tr>				<td colspan="2"><input type="submit" name="save" value="Save" />					&nbsp;&nbsp; <input type="reset" name="reset" value="Reset" /></td>			</tr>		</table>	</form:form></body></html>